+package com.ycs.order;
+
+import android.content.res.Resources;
+import android.graphics.drawable.Drawable;
+import android.support.v4.app.FragmentManager;
+import android.support.v4.app.FragmentTransaction;
+import android.support.v4.app.Fragment;
+import android.support.v7.app.AppCompatActivity;
+import android.os.Bundle;
+import android.view.View;
+import android.widget.TextView;
+
+import java.util.ResourceBundle;
+
+public class MainActivity extends AppCompatActivity {
+
+    @Override
+    protected void onCreate(Bundle savedInstanceState) {
+        super.onCreate(savedInstanceState);
+        setContentView(R.layout.activity_main);
+        getSupportActionBar().hide();
+        TextView maintv=(TextView)findViewById(R.id.main);
+        maintv.setOnClickListener(l);
+        TextView carttv=(TextView)findViewById(R.id.cart);
+        carttv.setOnClickListener(l);
+        TextView metv=(TextView)findViewById(R.id.me);
+        metv.setOnClickListener(l);
+    }
+
+    View.OnClickListener l=new View.OnClickListener() {
+        @Override
+        public void onClick(View v) {
+
+            FragmentManager fm=getSupportFragmentManager();
+            FragmentTransaction ft=fm.beginTransaction();
+            Fragment f=null;
+            TextView tvm=(TextView)findViewById(R.id.main);
+            TextView tv1=(TextView)findViewById(R.id.text1);
+            TextView tvc=(TextView)findViewById(R.id.cart);
+            TextView tv2=(TextView)findViewById(R.id.text2);
+            TextView tvme=(TextView)findViewById(R.id.me);
+            TextView tv3=(TextView)findViewById(R.id.text3);
+            switch (v.getId()){
+                case R.id.main:
+                    tvc.setBackgroundResource(R.mipmap.car);
+                    tvm.setBackgroundResource(R.mipmap.main2);
+                    tvme.setBackgroundResource(R.mipmap.me);
+                    f=new Main_Framgment();
+                    break;
+                case R.id.cart:
+                    tvc.setBackgroundResource(R.mipmap.car2);
+                    tvm.setBackgroundResource(R.mipmap.main);
+                    tvme.setBackgroundResource(R.mipmap.me);
+                    f=new cart_fragment();
+                    break;
+                case R.id.me:
+                    tvc.setBackgroundResource(R.mipmap.car);
+                    tvm.setBackgroundResource(R.mipmap.main);
+                    tvme.setBackgroundResource(R.mipmap.me2);
+                    f=new Me_Fragment();
+                    break;
+                default:
+                    break;
+            }
+            ft.replace(R.id.fragment, f);
+
+            ft.commit();
+
+        }
+    };
+}
